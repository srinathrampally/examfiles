<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".branch1">
    <TextView
        android:id="@+id/textView"
        android:layout_width="match_parent"
        android:layout_height="55dp"
        android:background="#1A9DDA"
        android:paddingTop="5sp"
        android:text="Branch"
        android:textAlignment="viewStart"
        android:textColor="#263238"
        android:textColorLink="#288134"
        android:textSize="30sp"
        android:textStyle="bold|italic"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/recyclerView"
        android:layout_width="409dp"
        android:layout_height="686dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        app:layout_constraintVertical_bias="0.0" />

    <androidx.cardview.widget.CardView
        android:id="@+id/cardViewAiml"
        android:layout_width="match_parent"
        android:layout_height="80dp"
        app:cardCornerRadius="8dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView">

        <TextView
            android:id="@+id/textView2"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:fontFamily="@font/akaya_telivigala"
            android:text="AIML"
            android:textAlignment="center"
            android:textSize="50sp"
            android:textStyle="bold|italic" />
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/CardViewCivil"
        android:layout_width="match_parent"
        android:layout_height="80dp"
        app:cardCornerRadius="8dp"
        app:layout_constraintEnd_toEndOf="@+id/cardViewAiml"
        app:layout_constraintStart_toStartOf="@+id/cardViewAiml"
        app:layout_constraintTop_toBottomOf="@+id/cardViewAiml" >

        <TextView
            android:id="@+id/textView3"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:fontFamily="@font/akaya_telivigala"
            android:text="CIVIL"
            android:textAlignment="center"
            android:textSize="50sp"
            android:textStyle="bold|italic" />
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/CardViewCse"
        android:layout_width="match_parent"
        android:layout_height="80dp"
        app:cardCornerRadius="8dp"
        app:layout_constraintEnd_toEndOf="@+id/CardViewCivil"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/CardViewCivil" >

        <TextView
            android:id="@+id/textView4"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:fontFamily="@font/akaya_telivigala"
            android:text="CSE"
            android:textAlignment="center"
            android:textSize="50sp"
            android:textStyle="bold|italic"/>
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/CardViewCsbs"
        android:layout_width="match_parent"
        android:layout_height="80dp"
        app:cardCornerRadius="8dp"
        app:layout_constraintEnd_toEndOf="@+id/CardViewCse"
        app:layout_constraintStart_toStartOf="@+id/CardViewCse"
        app:layout_constraintTop_toBottomOf="@+id/CardViewCse"

        >

        <TextView
            android:id="@+id/textView5"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:fontFamily="@font/akaya_telivigala"
            android:text="CSBS"
            android:textAlignment="center"
            android:textSize="50sp"
            android:textStyle="bold|italic" />
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/CardViewCsds"
        android:layout_width="match_parent"
        android:layout_height="80dp"
        app:cardCornerRadius="8dp"
        app:layout_constraintEnd_toEndOf="@+id/CardViewCsbs"
        app:layout_constraintStart_toStartOf="@+id/CardViewCsbs"
        app:layout_constraintTop_toBottomOf="@+id/CardViewCsbs" >

        <TextView
            android:id="@+id/textView7"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:fontFamily="@font/akaya_telivigala"
            android:text="CSDS"
            android:textAlignment="center"
            android:textSize="50sp"
            android:textStyle="bold|italic" />
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/CardViewIt"
        android:layout_width="match_parent"
        android:layout_height="80dp"
        app:cardCornerRadius="8dp"
        app:layout_constraintEnd_toEndOf="@+id/CardViewCsds"
        app:layout_constraintStart_toStartOf="@+id/CardViewCsds"
        app:layout_constraintTop_toBottomOf="@+id/CardViewCsds" >

        <TextView
            android:id="@+id/textView8"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:fontFamily="@font/akaya_telivigala"
            android:text="IT"
            android:textAlignment="center"
            android:textSize="50sp"
            android:textStyle="bold|italic" />
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/CardViewEce"
        android:layout_width="match_parent"
        android:layout_height="80dp"
        app:cardCornerRadius="8dp"
        app:layout_constraintEnd_toEndOf="@+id/CardViewIt"
        app:layout_constraintStart_toStartOf="@+id/CardViewIt"
        app:layout_constraintTop_toBottomOf="@+id/CardViewIt" >

        <TextView
            android:id="@+id/textView9"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:fontFamily="@font/akaya_telivigala"
            android:text="ECE"
            android:textAlignment="center"
            android:textSize="50sp"
            android:textStyle="bold|italic" />
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/CardViewEee"
        android:layout_width="match_parent"
        android:layout_height="80dp"
        app:cardCornerRadius="8dp"
        app:layout_constraintEnd_toEndOf="@+id/CardViewEce"
        app:layout_constraintStart_toStartOf="@+id/CardViewEce"
        app:layout_constraintTop_toBottomOf="@+id/CardViewEce" >

        <TextView
            android:id="@+id/textView10"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:fontFamily="@font/akaya_telivigala"
            android:text="EEE"
            android:textAlignment="center"
            android:textSize="50sp"
            android:textStyle="bold|italic" />
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/CardViewMech"
        android:layout_width="match_parent"
        android:layout_height="80dp"
        app:cardCornerRadius="8dp"
        app:layout_constraintEnd_toEndOf="@+id/CardViewEee"
        app:layout_constraintStart_toStartOf="@+id/CardViewEee"
        app:layout_constraintTop_toBottomOf="@+id/CardViewEee" >

        <TextView
            android:id="@+id/textView11"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:fontFamily="@font/akaya_telivigala"
            android:text="MECH"
            android:textAlignment="center"
            android:textSize="50sp"
            android:textStyle="bold|italic" />
    </androidx.cardview.widget.CardView>

</androidx.constraintlayout.widget.ConstraintLayout>